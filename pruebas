 public static Card DeckCard() {
        Random rand = new Random();
        Card c = new Card();

        while (c.num == 0) {
            c.num = rand.nextInt(9);
        }

        int colors = rand.nextInt(4);

        if (colors == 0) c.color = "blue";
        if (colors == 1) c.color = "red";
        if (colors == 2) c.color = "yellow";
        if (colors == 3) c.color = "green";

        return (c);

    }